{
  "name": "krixon/php-saml-client",
  "description": "SAML client for PHP",
  "license": "MIT",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Krixon\\SamlClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Krixon\\SamlClient\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "ext-curl": "*",
    "ext-date": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-zlib": "*",
    "psr/http-message": "^1.0",
    "robrichards/xmlseclibs": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "guzzlehttp/psr7": "^1.4"
  },
  "config": {
    "sort-packages": true
  }
}
